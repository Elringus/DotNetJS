{
    "name": "dotnet-runtime",
    "version": "0.4.1",
    "description": "JavaScript runtime for .NET",
    "keywords": [
        "csharp",
        "C#",
        "dotnet",
        "runtime",
        "WebAssembly",
        "interop",
        "ESM"
    ],
    "repository": "https://github.com/Elringus/DotNetJS",
    "license": "MIT",
    "type": "module",
    "files": [
        "dist/*"
    ],
    "source": "src/dotnet.ts",
    "main": "dist/dotnet.mjs",
