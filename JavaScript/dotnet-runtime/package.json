        "js-base64": "^3.7.2",
        "mocha": "^10.0.0",
        "nyc": "^15.1.0",
        "rollup": "^2.72.1",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-dts": "^4.2.1",
        "rollup-plugin-esbuild": "^4.9.1",
        "rollup-plugin-node-resolve": "^5.2.0",
        "typescript": "^4.6.4",
        "ws": "^8.6.0"
    }
