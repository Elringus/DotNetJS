<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <LangVersion>10</LangVersion>
        <LibraryName>packed</LibraryName>
        <Clean>false</Clean>
        <EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
        <CompilerGeneratedFilesOutputPath>debug</CompilerGeneratedFilesOutputPath>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="../../../DotNetJS/DotNetJS.csproj" OutputItemType="Analyzer"/>
        <Compile Remove="$(CompilerGeneratedFilesOutputPath)/*/**/*.cs"/>
    </ItemGroup>

</Project>
